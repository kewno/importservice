mixin searchElem(id, headline, text, pagination)
  .search-elem
    h2.search-elem__headline!= id+ `. ` + headline
    p.search-elem__text!= text
    .search-elem-pagination
      .search-elem-pagination__wrap-elems
        each el in pagination
          a(href=`${el.href}`).search-elem-pagination-elem= el.text
          <svg width="7" height="12" viewBox="0 0 7 12" fill="none" xmlns="http://www.w3.org/2000/svg">
          <path d="M1 1L5.29289 5.29289C5.68342 5.68342 5.68342 6.31658 5.29289 6.70711L1 11" stroke="white" stroke-linecap="round" stroke-linejoin="round"/>
          </svg>


          //pages-pagination
          //pages-pagination-elem
